filetype off
syntax off
filetype plugin indent on

imap jk <esc>
vmap ht <esc>

noremap <C-H> <C-W>h<C-W>_
noremap <C-L> <C-W>l<C-W>_
noremap <C-J> <C-W>j<C-W>_
noremap <C-K> <C-W>k<C-W>_


set tabstop=2
set shiftwidth=2
set expandtab

set hlsearch

set smarttab
set number

set t_Co=0
set backspace=indent,eol,start

set shell=/bin/bash\ -i

set ruler

set guifont=Consolas:h10
